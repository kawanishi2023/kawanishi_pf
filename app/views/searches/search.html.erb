<h2 class="css_font">検索結果</h2>

<table class="table table-hover table-inverse">
    <tbody>
      <% @songs.each do |song| %>
        <tr>
          <td><%= song.name %></td>
          <td><%= song.artist %></td>
          <td><%=link_to "詳細ページへ", song_path(song.id) %></td>
        </tr>
      <% end %>
    </tbody>
</table>