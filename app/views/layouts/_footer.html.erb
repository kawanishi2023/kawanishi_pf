

<footer class="footer mt-auto py-3" style="background-color: #EEEEEE;">
<div class="container">
<% if admin_signed_in? %> <!--adminログイン状態の際に、表示したいheaderのメニューを記載-->
<span class="nes-text is-primary css_font">Admin</span>
<% elsif user_signed_in? %> <!--userログイン状態の際に、表示したいheaderのメニューを記載-->
<span class="nes-text is-primary css_font">Menber</span>
<% else %><!--adminでもuserでもログインしていない時に表示するheaderのメニューを記載-->
<span class="nes-text is-primary css_font">Guest</span>
<% end %>
</div>
</footer>